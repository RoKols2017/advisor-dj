[req]
default_bits = 2048
prompt = no
default_md = sha256
x509_extensions = v3_req
distinguished_name = dn

[dn]
C = RU
ST = Local
L = Local
O = Advisor DJ
OU = LAN Test
CN = printadvisor.local

[v3_req]
keyUsage = critical, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = printadvisor.local
DNS.2 = localhost
IP.1 = 127.0.0.1
