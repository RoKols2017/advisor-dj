# Copy to /etc/default/advisor-ingest and adjust values.
INBOX_ROOT=/srv/advisor/inbox
WATCH_DIR=/var/lib/docker/volumes/advisor-dj_data/_data/watch
FAILED_DIR=/srv/advisor/inbox/_failed
ARCHIVE_DIR=/srv/advisor/inbox/_archive
STATE_DIR=/srv/advisor/ingest/state
LOG_FILE=/srv/advisor/ingest/logs/ingest_mover.log
LOCK_FILE=/var/lock/advisor-ingest.lock
MIN_AGE_SECONDS=20
MAX_FILES_PER_RUN=500
SOURCES="dc print1 print2"
